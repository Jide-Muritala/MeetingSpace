<div class="container">
  <h1><%= "Review your stay at #{@booking.office.name}"%></h1>
  <%= simple_form_for [@booking, @review] do |f| %>
    <%= f.input :rating, collection: (1..5).to_a, label: "Rate your experience" %>
    <%= f.input :comment %>
    <%= f.submit "Submit review", class: "btn btn-primary" %>
  <% end %>
</div>
