<div class="show-booking">
  <% unless @office.user == current_user %>
    <%= simple_form_for [@office, @booking], data: { controller: 'flatpickr' } do |f| %>
      <%= f.input :start_date, as: :string, label: false, input_html: { data: { flatpickr_target: "startDate" }, class: "range_start d-none" } %>
      <%= f.input :end_date, as: :string, label: false, input_html: { data: { flatpickr_target: "endDate" }, class: "range_end d-none" } %>
      <%= f.submit class: "btn-confirm" %>
    <% end %>
  <% end %>
</div>
